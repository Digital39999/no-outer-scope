{
	"version": "1.0.0",
	"name": "eslint-plugin-no-outer-scope",
	"author": "DIgital39999",
	"scripts": {
		"ts": "pnpm install typescript --save-dev -g",
		"init": "pnpm install && pnpm run build && pnpm run start",
